{
  "name": "riftea",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "generate": "npx prisma generate",
    "build": "npm run generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prisma": "prisma",
    "studio": "prisma studio",
    "dev:local": "npx dotenv -e .env.local -- next dev",
    "build:local": "npx dotenv -e .env.local -- next build",
    "start:local": "npx dotenv -e .env.local -- next start",
    "studio:local": "npx dotenv -e .env.local -- npx prisma studio",
    "migrate:local": "npx dotenv -e .env.local -- npx prisma migrate dev",
    "generate:local": "npx dotenv -e .env.local -- npx prisma generate",
    "dev:production": "npx dotenv -e .env.production -- next dev",
    "build:production": "npx dotenv -e .env.production -- next build",
    "start:production": "npx dotenv -e .env.production -- next start",
    "studio:production": "npx dotenv -e .env.production -- npx prisma studio",
    "migrate:production": "npx dotenv -e .env.production -- npx prisma migrate deploy",
    "generate:production": "npx dotenv -e .env.production -- npx prisma generate",
    "env:which:local": "npx dotenv -e .env.local -- node -e \"console.log('DATABASE_URL=', process.env.DATABASE_URL)\"",
    "env:which:production": "npx dotenv -e .env.production -- node -e \"console.log('DATABASE_URL=', process.env.DATABASE_URL)\""
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.1",
    "@supabase/supabase-js": "^2.57.0",
    "@vercel/blob": "^2.0.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.0",
    "canvas-confetti": "^1.9.3",
    "dotenv": "^17.2.1",
    "formidable": "^3.5.4",
    "html2canvas": "^1.4.1",
    "ioredis": "^5.7.0",
    "next": "^15.5.2",
    "next-auth": "^4.24.11",
    "node-cron": "^4.2.1",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.34.3",
    "uuid": "^11.1.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "24.2.1",
    "@types/react": "19.1.9",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.16.1",
    "tailwindcss": "^4"
  }
}

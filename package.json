{
  "name": "riftea",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "generate": "npx prisma generate",
    "build": "npm run generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prisma": "prisma",
    "studio": "prisma studio",
    "dev:docker": "npx dotenv --override -e .env.shared -e .env.docker -- next dev",
    "build:docker": "npx dotenv --override -e .env.shared -e .env.docker -- next build",
    "start:docker": "npx dotenv --override -e .env.shared -e .env.docker -- next start",
    "studio:docker": "npx dotenv --override -e .env.shared -e .env.docker -- npx prisma studio",
    "migrate:docker": "npx dotenv --override -e .env.shared -e .env.docker -- npx prisma migrate dev",
    "generate:docker": "npx dotenv --override -e .env.shared -e .env.docker -- npx prisma generate",
    "dev:supabase": "npx dotenv --override -e .env.shared -e .env.supabase -- next dev",
    "build:supabase": "npx dotenv --override -e .env.shared -e .env.supabase -- next build",
    "start:supabase": "npx dotenv --override -e .env.shared -e .env.supabase -- next start",
    "studio:supabase": "npx dotenv --override -e .env.shared -e .env.supabase -- npx prisma studio",
    "migrate:supabase": "npx dotenv --override -e .env.shared -e .env.supabase -- npx prisma migrate deploy",
    "generate:supabase": "npx dotenv --override -e .env.shared -e .env.supabase -- npx prisma generate",
    "env:which:docker": "npx dotenv --override -e .env.shared -e .env.docker -- node -e \"console.log('DATABASE_URL=', process.env.DATABASE_URL)\"",
    "env:which:supabase": "npx dotenv --override -e .env.shared -e .env.supabase -- node -e \"console.log('DATABASE_URL=', process.env.DATABASE_URL)\""
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.1",
    "@supabase/supabase-js": "^2.57.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.0",
    "dotenv": "^17.2.1",
    "formidable": "^3.5.4",
    "ioredis": "^5.7.0",
    "next": "^15.5.2",
    "next-auth": "^4.24.11",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.34.3",
    "uuid": "^11.1.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "24.2.1",
    "@types/react": "19.1.9",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.16.1",
    "tailwindcss": "^4"
  }
}
